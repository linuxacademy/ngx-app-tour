{
  "name": "ngx-tour",
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "app-tour",
    "ng-app-tour",
    "ngx-app-tour",
    "angular app tour",
    "angular app walkthrow"
  ],
  "author": "Hamdi Wanis <hamdiwanis@hotmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/hamdiwanis/ngx-app-tour"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "ng": "ng",
    "link": "cd dist/ngx-app-tour && npm link && cd ../..",
    "prestart": "npm run link",
    "start": "ng serve --aot=true",
    "prebuild": "npm run link",
    "build": "ng build --prod",
    "build:lib": "ng build ngx-app-tour --prod && npm run move:styles",
    "move:styles": "cpx projects/ngx-app-tour/src/lib/styles/styles.css dist/ngx-app-tour/styles",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pub": "npm run pub:lib && npm run pub:demo",
    "pub:demo": "npm run build --prod --base-href \"https://hamdiwanis.github.io/ngx-app-tour/\" && gh-pages -d dist/app",
    "pub:lib": "cd dist/ngx-app-tour && npm publish"
  },
  "dependencies": {
    "@angular/animations": "^7.1.1",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/forms": "^7.1.1",
    "@angular/http": "^7.1.1",
    "@angular/platform-browser": "^7.1.1",
    "@angular/platform-browser-dynamic": "^7.1.1",
    "@angular/router": "^7.1.1",
    "core-js": "^2.5.1",
    "rxjs": "^6.1.0",
    "withinviewport": "^2.0.0",
    "zone.js": "^0.8.18",
    "ngx-app-tour": "1.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "^7.1.0",
    "@angular/compiler": "^7.1.1",
    "@angular/compiler-cli": "^7.1.1",
    "@types/jasmine": "^3.3.0",
    "@types/node": "^10.12.10",
    "codelyzer": "^4.0.1",
    "cpx": "^1.5.0",
    "gh-pages": "^2.0.1",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^10.1.1",
    "protractor": "^5.3.2",
    "ts-node": "^7.0.1",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "~3.1.6"
  }
}
